{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2020",
    "rootDir": "src/",
    "baseUrl": "src/",
    "paths": {
      "/lib/*": [ "lib/*" ],
      "/logger/*": [ "logger/*" ],
      "/help/*": [ "help/*" ],
      "/cron/*": [ "cron/*" ]
    },
    "outDir": "build/",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "strictPropertyInitialization": true
  }
}